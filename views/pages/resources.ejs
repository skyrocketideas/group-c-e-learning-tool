<!DOCTYPE html>
<html lang="en">
	<head>
		<%- include ("../partials/header.ejs") %>
	</head>
	<body>
		<h1>Educational Resources</h1>
		<ul>
			<% resources.forEach(function(resource) { %>
			<li><h2><%= resource.name %></h2></li>
			<li><h3><%= resource.description %></h3></li>
			<li><%= resource.text %></li>
			<li><%= resource.external_link %></li>
			<hr />
			<% }); %>
		</ul>
	</body>
</html>
